{
  "name": "hashi",
  "version": "1.0.0",
  "description": "A web application that integrates with UCSF ChimeraX for molecular visualization",
  "main": "dist/server/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev:server": "nodemon src/server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:server": "tsc",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:backend": "jest --config=jest.config.js",
    "test:client": "jest --config=jest.client.config.js",
    "test:security": "jest --testPathPattern=tests/security",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:perf": "k6 run tests/performance/session-api-load.js",
    "test:all": "npm run test:backend && npm run test:client && npm run test:security && npm run test:e2e",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "chimerax",
    "molecular-visualization",
    "web-app"
  ],
  "author": "Gomes Group",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^10.0.4",
    "@react-three/fiber": "^9.1.0",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pg": "^8.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.15",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "three": "^0.174.0",
    "typeorm": "^0.3.21",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-playwright": "^2.0.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-playwright": "^1.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^23.0.0",
    "msw": "^2.5.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "jest-junit": {
    "outputDirectory": "./test-results",
    "outputName": "junit.xml"
  }
}
